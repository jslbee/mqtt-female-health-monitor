    "health",
    "women",
    "monitoring"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vueuse/core": "^13.1.0",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "vite": "^6.3.5",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
